{
  "name": "@2f/vue-components",
  "version": "1.0.0",
  "description": "2Fresh library with Vue components",
  "main": "./dist/2fresh__dev-vue-components.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name 2fresh__dev-vue-components ./src/components/index.js",
    "lint": "vue-cli-service lint",
    "push-npm": "yarn build-bundle"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/2fresh/2fresh__dev-vue-components.git"
  },
  "keywords": [
    "vue",
    "components"
  ],
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/2fresh/2fresh__dev-vue-components#readme",
  "dependencies": {
    "core-js": "^2.6.5",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.10"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
